{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@rahino/core": ["apps/core/src"],
      "@rahino/core/*": ["apps/core/src/*"],
      "@rahino/database": ["libs/database/src"],
      "@rahino/database/*": ["libs/database/src/*"],
      "@rahino/http-exception-filter": ["libs/http-exception-filter/src"],
      "@rahino/http-exception-filter/*": ["libs/http-exception-filter/src/*"],
      "@rahino/logger": ["libs/logger/src"],
      "@rahino/logger/*": ["libs/logger/src/*"],
      "@rahino/auth": ["libs/auth/src"],
      "@rahino/auth/*": ["libs/auth/src/*"],
      "@rahino/response": ["libs/response/src"],
      "@rahino/response/*": ["libs/response/src/*"],
      "@rahino/query-filter": ["libs/query-filter/src"],
      "@rahino/query-filter/*": ["libs/query-filter/src/*"],
      "@rahino/commonmiddleware": ["libs/commonmiddleware/src"],
      "@rahino/commonmiddleware/*": ["libs/commonmiddleware/src/*"],
      "@rahino/commontools": ["libs/commontools/src"],
      "@rahino/commontools/*": ["libs/commontools/src/*"],
      "@rahino/neka": ["apps/neka/src"],
      "@rahino/neka/*": ["apps/neka/src/*"]
    }
  }
}
